{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "labels": {
      "app": "alpine"
    },
    "name": "alpine"
  },
  "spec": {
    "containers": [
      {
        "command": [
          "sleep",
          "3600"
        ],
        "image": "alpine",
        "imagePullPolicy": "IfNotPresent",
        "name": "busybox"
      }
    ],
    "restartPolicy": "Always"
  }
}